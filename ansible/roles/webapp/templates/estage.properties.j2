# parametres des URLs pour l'authentication CAS
cas.url.login   ={{ ESTAGE_ENVIRONMENT.passeport_url | default('https://passeport.dauphine.fr/cas',true }}/login?service={service}
cas.url.service ={{ ESTAGE_ENVIRONMENT.passeport_url | default('https://passeport.dauphine.fr/cas',true }}/p3/serviceValidate?service={service}&ticket={ticket}&format=json
